{
  "version": 2,
  "builds": [
    {
      "src": "adriel-maas-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [

    {
      "src": "/(.*)",
      "headers": { "cache-control": "no-cache" },
      "dest": "/index.html"
    }

  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  }
}
